            BindName.ZIndex = 3
            BindName.Font = Enum.Font.GothamBold
            BindName.Text = OldBind
            BindName.TextColor3 = Theme.TextColor
            BindName.TextSize = 14.000
            BindName.TextWrapped = true
            ClickEffect(BindExampleButton)

            BindExampleButton.MouseButton1Click:connect(function(e) 
                BindName.Text = "..."
                local a, b = game:GetService('UserInputService').InputBegan:wait();
                if a.KeyCode.Name ~= "Unknown" then
                    BindName.Text = a.KeyCode.Name
                    OldBind = a.KeyCode.Name;
                end
            end)
        
            game:GetService("UserInputService").InputBegan:connect(function(Key, Nope)
                if not Nope then 
                    if Key.KeyCode.Name == OldBind then
                        pcall(callback())
                    end
                end
            end)
        end

        function ExtraLibrary:CreateColourPicker(text, DefaultColor, Callback)
            local Data = {}
            local Enabled = true;
            Default = DefaultColor or Color3.fromRGB(255,255,255)

            local ColorFrame = Instance.new("ImageLabel")
