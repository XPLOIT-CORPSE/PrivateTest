        local ExtraLibrary  = {}

        function ExtraLibrary:CreateToggle(text, callback)
            local actions = {}
            local enabled = false

            local ToggleExample = Instance.new("TextLabel")
            local ToggleExampleExample_Background = Instance.new("ImageLabel")
            local ToggleButton = Instance.new("ImageButton")
            local ToggleButtonRound = Instance.new("ImageLabel")

            ToggleExample.Name = "ToggleExample"
            ToggleExample.Parent = ExampleTab
            ToggleExample.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
            ToggleExample.BackgroundTransparency = 1.000
            ToggleExample.BorderSizePixel = 0
            ToggleExample.Position = UDim2.new(0.0536923148, 0, 0.373801917, 0)
            ToggleExample.Size = UDim2.new(0, 438, 0, 30)
            ToggleExample.ZIndex = 2
            ToggleExample.Font = Enum.Font.GothamBold
            ToggleExample.Text = "  " ..text
            ToggleExample.TextColor3 = Theme.TextColor
            ToggleExample.TextSize = 13.000
            ToggleExample.TextXAlignment = Enum.TextXAlignment.Left

            ToggleExampleExample_Background.Name = "ToggleExampleExample_Background"
            ToggleExampleExample_Background.Parent = ToggleExample
            ToggleExampleExample_Background.AnchorPoint = Vector2.new(0.5, 0.5)
            ToggleExampleExample_Background.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            ToggleExampleExample_Background.BackgroundTransparency = 1.000
            ToggleExampleExample_Background.Position = UDim2.new(0.5, 0, 0.5, 0)
            ToggleExampleExample_Background.Size = UDim2.new(1, 0, 1, 0)
            ToggleExampleExample_Background.Image = "rbxassetid://3570695787"
            ToggleExampleExample_Background.ImageColor3 = Theme.ToggleExampleExample_Background 
            ToggleExampleExample_Background.ScaleType = Enum.ScaleType.Slice
            ToggleExampleExample_Background.SliceCenter = Rect.new(100, 100, 100, 100)
            ToggleExampleExample_Background.SliceScale = 0.050

            ToggleButton.Name = "ToggleButton"
            ToggleButton.Parent = ToggleExample
            ToggleButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            ToggleButton.BackgroundTransparency = 1.000
            ToggleButton.BorderSizePixel = 0
            ToggleButton.Size = UDim2.new(0, 438, 0, 30)
            ToggleButton.ZIndex = 4
            ClickEffect(ToggleButton)

            ToggleButtonRound.Name = "ToggleButtonRound"
            ToggleButtonRound.Parent = ToggleButton
            ToggleButtonRound.AnchorPoint = Vector2.new(0.5, 0.5)
            ToggleButtonRound.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            ToggleButtonRound.BackgroundTransparency = 1.000
            ToggleButtonRound.Position = UDim2.new(0.948940635, 0, 0.490999997, 0)
            ToggleButtonRound.Size = UDim2.new(0, 20, 0, 20)
            ToggleButtonRound.ZIndex = 3
            ToggleButtonRound.Image = "rbxassetid://3570695787"
            ToggleButtonRound.ImageColor3 = Theme.ToggleButtonRound 
            ToggleButtonRound.ScaleType = Enum.ScaleType.Slice
            ToggleButtonRound.SliceCenter = Rect.new(100, 100, 100, 100)
            ToggleButtonRound.SliceScale = 0.040

            local function Fire()
                enabled = not enabled
                if enabled then
                    game:GetService("TweenService"):Create(ToggleButtonRound, TweenInfo.new(0.2), {ImageColor3 = Color3.fromRGB(255, 255, 255)}):Play()
                end
                if not enabled then
                    game:GetService("TweenService"):Create(ToggleButtonRound, TweenInfo.new(0.2), {ImageColor3 = Color3.fromRGB(40, 40, 40)}):Play()
                end
                pcall(callback, enabled)
            end

            ToggleButton.MouseButton1Click:connect(Fire)

            function actions:Set(arg)
                if enabled then
                    game:GetService("TweenService"):Create(ToggleButtonRound, TweenInfo.new(0.2), {ImageColor3 = Color3.fromRGB(255, 255, 255)}):Play()
                end
                if not enabled  then
                    game:GetService("TweenService"):Create(ToggleButtonRound, TweenInfo.new(0.2), {ImageColor3 = Color3.fromRGB(40, 40, 40)}):Play()
                end
                pcall(callback, arg)
            end

        end


