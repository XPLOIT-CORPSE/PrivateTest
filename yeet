        ExampleTab.Size = UDim2.new(0, 472, 0, 317)
        ExampleTab.ZIndex = 3
        ExampleTab.ScrollBarThickness = 0
        ExampleTab.Visible = false

        UIListLayoutTab:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
            ExampleTab.CanvasSize = UDim2.new(0,0,0, UIListLayoutTab.AbsoluteContentSize.Y + 20)
        end)

        ExampleTab.ChildAdded:connect(function()
            ExampleTab.CanvasSize = UDim2.new(0,0,0, UIListLayoutTab.AbsoluteContentSize.Y + 20)
        end)
        ExampleTab.ChildRemoved:connect(function()
            ExampleTab.CanvasSize = UDim2.new(0,0,0, UIListLayoutTab.AbsoluteContentSize.Y + 20)
        end)

        
        UIPaddingTab.Name = "UIPaddingTab"
        UIPaddingTab.Parent = ExampleTab
