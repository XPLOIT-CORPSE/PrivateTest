            local RainbowToggleButton = Instance.new("TextButton")
            local RainbowToggleState = Instance.new("ImageLabel")
            local RainbowLabel = Instance.new("TextLabel")
            local PickerFrame = Instance.new("ImageLabel")
            local ActualPicker = Instance.new("ImageButton")
            local Marker = Instance.new("ImageLabel")
            local ActualValue = Instance.new("ImageButton")
            local PickerValue = Instance.new("Frame")
            
