function UI.Settings(Set)
	local Title = Set.Title or "UiLibrary"
	local Theme = Set.Theme or "DarkDarker"
	local ChangeColor = Set.ColorOverrides or {}

	Theme = Themes[Theme]

	SelectedTheme = Theme

	for KeyChange, ValueChange in next, ChangeColor do
		SelectedTheme[KeyChange] = ValueChange
	end

    local random = Random.new()
    local letters = {'a','b','c','d','e','f','g','h','i','j','k','l','m','n','o','p','q','r','s','t','u','v','w','x','y','z'}

    function getRandomLetter()
        return letters[random:NextInteger(1,#letters)]
    end

    function getRandomString(length, includeCapitals)
        local length = length or 10
        local str = ''
        for i=1,length do
            local randomLetter = getRandomLetter()
            if includeCapitals and random:NextNumber() > .5 then
                randomLetter = string.upper(randomLetter)
            end
            str = str .. randomLetter
        end
        return str
    end

    for i,v in pairs(game.CoreGui:GetChildren()) do
        if UiLibv3 then
            v:Destroy()
        end
    end

    local UiLibv3 = Instance.new("ScreenGui")
    UiLibv3.Name = getRandomString(10, true)
    UiLibv3.Parent = game.CoreGui

    local TopFrame = Instance.new("ImageLabel")
    local MenuButton = Instance.new("ImageButton")
    local ListFrameHolder = Instance.new("ImageLabel")
