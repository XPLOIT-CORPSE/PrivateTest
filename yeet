            ActualColor.ClipsDescendants = true
            ActualColor.Position = UDim2.new(0.049788136, 0, 0.490259737, 0)
            ActualColor.Size = UDim2.new(0, 425, 0, 0)
            ActualColor.Image = "rbxassetid://3570695787"
            ActualColor.ImageColor3 = Theme.ActualColor 
            ActualColor.ScaleType = Enum.ScaleType.Slice
            ActualColor.SliceCenter = Rect.new(100, 100, 100, 100)
            ActualColor.SliceScale = 0.050
            
            RainbowToggleFrame.Name = "RainbowToggleFrame"
