            ActualPicker.Parent = PickerFrame
            ActualPicker.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            ActualPicker.Position = UDim2.new(0.0407348163, 0, 0.0509916022, 0)
            ActualPicker.Size = UDim2.new(0.904450476, 1, 0.861008763, 1)
            ActualPicker.Image = "rbxassetid://1433361550"
            
            Marker.Name = "Marker"
            Marker.Parent = ActualPicker
            Marker.AnchorPoint = Vector2.new(0.5, 0.5)
            Marker.BackgroundColor3 = Color3.fromRGB(83, 83, 83)
            Marker.BackgroundTransparency = 1.000
            Marker.BorderColor3 = Color3.fromRGB(255, 255, 255)
            Marker.Position = UDim2.new(0.00520178862, 1, 0.948624432, 1)
            Marker.Size = UDim2.new(0, 5, 0, 5)
            Marker.ZIndex = 5
            Marker.Image = "rbxassetid://3570695787"
            Marker.ImageColor3 = Color3.fromRGB(27, 27, 27)
            Marker.ScaleType = Enum.ScaleType.Slice
            Marker.SliceCenter = Rect.new(100, 100, 100, 100)
            
            ActualValue.Name = "ActualValue"
            ActualValue.Parent = Marker
            ActualValue.BackgroundColor3 = Color3.fromRGB(27, 27, 27)
            ActualValue.BackgroundTransparency = 1.000
            ActualValue.BorderSizePixel = 0
            ActualValue.ClipsDescendants = true
            ActualValue.Position = UDim2.new(0.796862781, 0, 0.0756652802, 0)
            ActualValue.Size = UDim2.new(0.172477648, 1, 0.896222293, 1)
            ActualValue.ZIndex = 3
            ActualValue.Image = "rbxassetid://359311684"
            ActualValue.ImageColor3 = Color3.fromRGB(27, 27, 27)
            
            PickerValue.Name = "PickerValue"
            PickerValue.Parent = ActualValue
            PickerValue.BackgroundColor3 = Color3.fromRGB(27, 27, 27)
            PickerValue.BackgroundTransparency = 1.000
            PickerValue.BorderColor3 = Color3.fromRGB(0, 0, 0)
            PickerValue.BorderSizePixel = 0
            PickerValue.Position = UDim2.new(-0.200207323, 0, 0, 0)
            PickerValue.Size = UDim2.new(0, 30, 0, 4)
            PickerValue.ZIndex = 5

            local Mouse = game:GetService("Players").LocalPlayer:GetMouse()

            function HSVtoRGB(hsv)
                local color3 = hsv
                local r = math.floor(color3.R * 255)
                local g = math.floor(color3.G * 255)
                local b = math.floor(color3.B * 255)
                return r,g,b
            end
            
            ColorButton.MouseButton1Down:connect(function()
                if ActualColor.Size == UDim2.new(0, 425,0, 124) then
                    ActualColor:TweenSize(UDim2.new(0, 425,0, 0), "InOut", "Quad", 0.5, true)
                elseif ActualColor.Size == UDim2.new(0, 425,0, 0) then
                    ActualColor:TweenSize(UDim2.new(0, 425,0, 124), "InOut", "Quad", 0.5, true)
                end
            end)
        
            function SetColor(hue,sat,val)
                Data = {hue or Data[1],sat or Data[2],val or 1}
                ColorState.ImageColor3 = Color3.fromHSV(Data[1],Data[2],Data[3])
                ActualValue.ImageColor3 = Color3.fromHSV(Data[1],Data[2],1)
                local R,G,B = HSVtoRGB(Color3.fromHSV(Data[1],Data[2],Data[3]))
                Callback(Color3.fromRGB(R,G,B))
            end
        
            RainbowToggleButton.MouseButton1Down:connect(function()
                if Enabled then
                    game:GetService("TweenService"):Create(RainbowToggleState, TweenInfo.new(0.2, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {ImageColor3 = (Color3.fromRGB(56, 56, 56))}):Play()
                else
                    game:GetService("TweenService"):Create(RainbowToggleState, TweenInfo.new(0.2, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {ImageColor3 = (Color3.fromRGB(255, 255, 255))}):Play()
                end
                Enabled = not Enabled
                spawn(function()
                    while wait() and Enabled do
                        local H = tick()%5/5
                        local S = 0.75
                        local V = 0.75 
                        Marker:TweenPosition(UDim2.new(H,1,S,1),"InOut","Sine",0.04,true)
                        SetColor(H,S,V)        
                    end
                end)
            end)
        
            function GetXY(frame)
                local x,y = Mouse.X - frame.AbsolutePosition.X - 2,Mouse.Y - frame.AbsolutePosition.Y - 2
                local maxX,maxY = frame.AbsoluteSize.X,frame.AbsoluteSize.Y
                x = math.clamp(x, 0, maxX);
                y = math.clamp(y, 0, maxY);
                return x/maxX,y/maxY
            end
        
            function UpdateRGB()
                local x,y = GetXY(ActualPicker)
                if x and y then
                    Marker:TweenPosition(UDim2.new(x,1,y,1),"InOut","Sine",0.04,true)
                    SetColor(1 - x,1 - y)
                end
            end
        
            function UpdateValue()
                local x,y = GetXY(PickerValue.Parent)
                if x and y then
                    PickerValue:TweenPosition(UDim2.new(0,0,y,-3.5),"InOut","Sine",0.04,true)
                    SetColor(nil,nil,1 - y)
                end
            end
        
            game:GetService("UserInputService").InputEnded:Connect(function(Input)
                if Input.UserInputType == Enum.UserInputType.MouseButton1 then
                    if Connect then
                        Connect:Disconnect()
                        Connect = nil
                    end
                end
            end)
        
            game:GetService("UserInputService").InputEnded:Connect(function(Input)
                if Input.UserInputType == Enum.UserInputType.MouseButton1 then
                    if Connect then
                        Connect:Disconnect()
                        Connect = nil
                    end
                end
            end)
        
            ActualPicker.MouseButton1Down:Connect(function()
                if Connect then
                    Connect = nil
                end
                Connect = game:GetService("RunService").Stepped:Connect(function()
                    UpdateRGB()
                end)
            end)
        
            ActualValue.MouseButton1Down:Connect(function()
                if Connect then
                    Connect = nil
                end
                Connect = game:GetService("RunService").Stepped:Connect(function()
                    UpdateValue()
                end)
            end)
        end
        return ExtraLibrary
    end
end
return UI
