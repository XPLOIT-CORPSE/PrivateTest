    CloseOpenButton.MouseButton1Click:connect(function()
        if BackGroundMain.Size == UDim2.new(0, 489,0, 340) then
            game:GetService("TweenService"):Create(CloseOpenButton, TweenInfo.new(0.25), {Rotation = 90}):Play()

            BackGroundMain:TweenSize(UDim2.new(0, 489,0, 0), "Out", "Quint", 0.8)
            print("closed")
        elseif BackGroundMain.Size == UDim2.new(0, 489,0, 0) then
            game:GetService("TweenService"):Create(CloseOpenButton, TweenInfo.new(0.25), {Rotation = 0}):Play()
            BackGroundMain:TweenSize(UDim2.new(0, 489,0, 340), "Out", "Quint", 0.7)
            wait(0.2)

            print("opened")
        end
    end)

    DestroyGUIButton.MouseButton1Click:connect(function()
        wait()
        MainSettingsFrame:TweenSize(UDim2.new(0, 0,0, 126), "Out", "Quint", 0.8)
        wait(1)
        BackGroundMain.ClipsDescendants = true
        BackGroundMain:TweenSize(UDim2.new(0, 489,0, 0), "Out", "Quint", 0.7)
        wait(0.6)
        TopFrame.ClipsDescendants = true
        TopFrame:TweenSize(UDim2.new(0, 489,0, 0), "Out", "Quint", 0.7)
        for i,v in pairs(TopFrame:GetChildren()) do 
            v.Visible = false
        end
        wait(1)
        for i,v in pairs(game.CoreGui:GetChildren()) do
            if v.Name == "UiLibv3" then
                v:Destroy()
            end
        end
    end)

    SettingsButton.MouseButton1Click:connect(function()
        if MainSettingsFrame.Size == UDim2.new(0, 258,0, 126) then
            MainSettingsFrame:TweenSize(UDim2.new(0, 0,0, 126), "Out", "Quint", 0.8)
            print("closed")
        elseif MainSettingsFrame.Size == UDim2.new(0, 0,0, 126) then
            MainSettingsFrame:TweenSize(UDim2.new(0, 258,0, 126), "Out", "Quint", 0.7)
            print("opened")
        end
    end)

    local function JCISG_fake_script() -- TopFrame.DraggableScript 
        local script = Instance.new('LocalScript', TopFrame)

        local UIS = game:GetService("UserInputService")
        function dragify(Frame)
            dragToggle = nil
            local dragSpeed = 0.30
            dragInput = nil
            dragStart = nil
            local dragPos = nil
            function updateInput(input)
                local Delta = input.Position - dragStart
                local Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
                game:GetService("TweenService"):Create(Frame, TweenInfo.new(0.25), {Position = Position}):Play()
            end
            Frame.InputBegan:Connect(function(input)
                if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and UIS:GetFocusedTextBox() == nil then
                    dragToggle = true
                    dragStart = input.Position
                    startPos = Frame.Position
                    input.Changed:Connect(function()
                        if input.UserInputState == Enum.UserInputState.End then
                            dragToggle = false
                        end
                    end)
                end
            end)
            Frame.InputChanged:Connect(function(input)
                if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
                    dragInput = input
                end
            end)
            game:GetService("UserInputService").InputChanged:Connect(function(input)
                if input == dragInput and dragToggle then
                    updateInput(input)
                end
            end)
        end
        dragify(script.Parent)
    end
    coroutine.wrap(JCISG_fake_script)()

    function ClickEffect(L_1_arg0)
        function CreateInstance(L_5_arg0, L_6_arg1)    
            local L_7_ = Instance.new(L_5_arg0);    
            for L_8_forvar0, L_9_forvar1 in pairs(L_6_arg1) do
                L_7_[L_8_forvar0] = L_9_forvar1
            end;
            return L_7_
        end;
        
        L_1_arg0.AutoButtonColor = false;
        L_1_arg0.ClipsDescendants = true;
        
        local L_3_ = CreateInstance("Frame", {
            Size = UDim2.new(1, 0, 1, 0),
            Parent = L_1_arg0,
            Name = "FX",

            AnchorPoint = Vector2.new(0, 0),
            BackgroundColor3 = L_2_,
            BorderSizePixel = 0,
            BackgroundTransparency = 1,
            Position = UDim2.new(0, 0, 0, 0),
            Visible = true,
            ZIndex = L_1_arg0.ZIndex + 1
        })

        local function L_4_func()
            local L_12_ = game:GetService('Players').LocalPlayer:GetMouse();
            local L_13_ = math.sqrt((L_1_arg0.Size.X.Offset) ^ 1.2 + (L_1_arg0.Size.Y.Offset) ^ 1.2)
            local L_14_ = (L_12_.X) - (L_1_arg0.AbsolutePosition.X);
            local L_15_ = (L_12_.Y) - (L_1_arg0.AbsolutePosition.Y);
            local L_16_ = CreateInstance('ImageLabel', {
                Name = "CircleLabel",
                Parent = L_1_arg0,
                AnchorPoint = Vector2.new(0.5, 0.5),
                BackgroundColor3 = Color3.new(1, 1, 1),
                BackgroundTransparency = 1,
                BorderSizePixel = 0,
                Position = UDim2.new(0, 0, 0, 0),
                Size = UDim2.new(0, 50, 0, 50),
                Visible = false,
                Image = "rbxasset://textures/whiteCircle.png",
                ImageTransparency = .40
            })
                    
            L_16_.Position = UDim2.new(0, L_14_, 0, L_15_);
            
            L_16_.Size = UDim2.new(0, 0, 0, 0);
            
            L_16_.ImageTransparency = .76;
            
            L_16_.Visible = true;
            
            L_16_.ZIndex = L_1_arg0.ZIndex + 2
            
            local L_17_ = UDim2.new(0, L_13_ * 2.15, 0, L_13_ * 2.15)
            
            L_1_arg0.InputEnded:Connect(function(L_18_arg0)
                if L_18_arg0.UserInputType == Enum.UserInputType.MouseMovement then
                    while wait() do
                        if L_16_.Size == L_17_ then
                            game:GetService("TweenService"):Create(L_16_, TweenInfo.new(.3, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0), {
                                ImageTransparency = 1
                            }):Play()
                            wait(1.2)
                            L_16_:Destroy()
                            break
                        end
                    end
                end
            end)

            L_1_arg0.MouseButton1Up:Connect(function()
                wait(0.17)
                game:GetService("TweenService"):Create(L_16_, TweenInfo.new(.3, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0), {
                    ImageTransparency = 1
                }):Play()
                wait(1.2)
                L_16_:Destroy()
            end)
            
            L_16_:TweenSize(L_17_, "Out", "Linear", 0.45)
        end
        
        L_1_arg0.MouseButton1Down:Connect(L_4_func);
    end;


    local Library = {}

    local folder = Instance.new("Folder", TopFrame)
    local inf = Instance.new("IntValue", TopFrame.Folder)
    inf.Name = "TabHolder"

    function Library:CreateTab(text, extratext)
        extratext = extratext or text
        local ExampleTabButton = Instance.new("TextButton")

        ExampleTabButton.Name = "ExampleTabButton"
        ExampleTabButton.Parent = ListFrame
        ExampleTabButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        ExampleTabButton.BackgroundTransparency = 1.000
        ExampleTabButton.Size = UDim2.new(0, 163, 0, 38)
        ExampleTabButton.ZIndex = 8
        ExampleTabButton.Font = Enum.Font.GothamBold
        ExampleTabButton.Text = text
        ExampleTabButton.TextColor3 = Theme.TextColor
        ExampleTabButton.TextSize = 13.000
        game:GetService("TweenService"):Create(ExampleTabButton, TweenInfo.new(0.5), {TextColor3 = Color3.fromRGB(169, 169, 169), TextSize = 13}):Play()
        
        local Items = MainTabFrame
        local Button1  = ExampleTabButton
        local Tabs = ListFrame

        ExampleTabButton.MouseButton1Click:Connect(function()
            for i,v2 in pairs(Items:GetChildren()) do
                if ExampleTabButton.Name ~= v2.Name then
                    v2.Visible = false
                else
                    v2.Visible = true
                end
            end
            for i,v3 in pairs (Tabs:GetChildren()) do
                if v3.ClassName == "TextButton" then
                    if ExampleTabButton.Name ~= v3.Name then
                        game:GetService("TweenService"):Create(v3, TweenInfo.new(0.5), {TextColor3 = Color3.fromRGB(169, 169, 169), TextSize = 13}):Play()
                    else
                        game:GetService("TweenService"):Create(v3, TweenInfo.new(0.5), {TextColor3 = Color3.fromRGB(255,255,255), TextSize = 13.5}):Play()
                    end
                end	
            end
        end)

        local ExampleTab = Instance.new("ScrollingFrame")
        local UIPaddingTab = Instance.new("UIPadding")
        local UIListLayoutTab = Instance.new("UIListLayout")

        ExampleTabButton.Name = TopFrame.Folder.TabHolder.Value +1
        TopFrame.Folder.TabHolder.Value = TopFrame.Folder.TabHolder.Value +1

        ExampleTab.Name = ExampleTabButton.Name
