        function ExtraLibrary:CreateSlider(text, min, max, start, callback)
            start = start or 0
            local SliderExample = Instance.new("TextButton")
            local SliderExample_Background = Instance.new("ImageLabel")
            local SliderValue = Instance.new("TextLabel")
            local Slider = Instance.new("ImageLabel")
            
            SliderExample.Name = "SliderExample"
            SliderExample.Parent = ExampleTab
            SliderExample.BackgroundColor3 = Color3.fromRGB(34, 34, 34)
            SliderExample.BackgroundTransparency = 1.000
            SliderExample.BorderSizePixel = 0
            SliderExample.Position = UDim2.new(0.0536923148, 0, 0.169329077, 0)
            SliderExample.Size = UDim2.new(0, 438, 0, 30)
            SliderExample.ZIndex = 2
            SliderExample.Font = Enum.Font.GothamBold
            SliderExample.Text = "  " ..text
            SliderExample.TextColor3 = Theme.TextColor
            SliderExample.TextSize = 13.000
            SliderExample.TextXAlignment = Enum.TextXAlignment.Left
            
            SliderExample_Background.Name = "SliderExample_Background"
            SliderExample_Background.Parent = SliderExample
            SliderExample_Background.Active = true
            SliderExample_Background.AnchorPoint = Vector2.new(0.5, 0.5)
            SliderExample_Background.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            SliderExample_Background.BackgroundTransparency = 1.000
            SliderExample_Background.Position = UDim2.new(0.5, 0, 0.5, 0)
            SliderExample_Background.Selectable = true
            SliderExample_Background.Size = UDim2.new(1, 0, 1, 0)
            SliderExample_Background.Image = "rbxassetid://3570695787"
            SliderExample_Background.ImageColor3 = Theme.SliderExample_Background 
            SliderExample_Background.ScaleType = Enum.ScaleType.Slice
            SliderExample_Background.SliceCenter = Rect.new(100, 100, 100, 100)
            SliderExample_Background.SliceScale = 0.050
            
            SliderValue.Name = "SliderValue"
            SliderValue.Parent = SliderExample
            SliderValue.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            SliderValue.BackgroundTransparency = 1.000
            SliderValue.Position = UDim2.new(0.7666049, 0, 0.174649805, 0)
            SliderValue.Size = UDim2.new(0, 90, 0, 18)
            SliderValue.ZIndex = 2
            SliderValue.Font = Enum.Font.GothamBold
            SliderValue.Text = tostring(start and math.floor((start / max) * (max - min) + min) or 0) .. "/" .. max
            SliderValue.TextColor3 = Theme.TextColor
            SliderValue.TextSize = 14.000
            SliderValue.TextWrapped = true
            SliderValue.TextXAlignment = Enum.TextXAlignment.Right
            
            Slider.Name = "Slider"
            Slider.Parent = SliderExample
            Slider.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Slider.BackgroundTransparency = 1.000
            Slider.Size = UDim2.new((start or 0) / max, 0, 1, 0)
            Slider.Image = "rbxassetid://3570695787"
            Slider.ImageColor3 = Theme.Slider 
            Slider.ScaleType = Enum.ScaleType.Slice
            Slider.SliceCenter = Rect.new(100, 100, 100, 100)
            Slider.SliceScale = 0.050

            local function slide(input)
                local pos = UDim2.new(math.clamp((input.Position.X - SliderExample.AbsolutePosition.X) / SliderExample.AbsoluteSize.X, 0, 1), 0, 1, 0);
                Slider:TweenSize(pos, Enum.EasingDirection.Out, Enum.EasingStyle.Quad, 0.2, true);
                local value = math.floor(((pos.X.Scale * max) / max) * (max - min) + min);
                SliderValue.Text = tostring(value) .. "/" .. max;
                callback(value);
            end;

            local slidng = false

            SliderExample.MouseEnter:Connect(function()
                slidng = true
            end)
            SliderExample.MouseLeave:Connect(function()
                slidng = false
            end)

            SliderExample.InputBegan:Connect(function(input)
                if slidng == true then
                    if input.UserInputType == Enum.UserInputType.MouseButton1 then
                        dragging = true;
                    end;
                end
            end);

            SliderExample.InputEnded:Connect(function(input)
                if input.UserInputType == Enum.UserInputType.MouseButton1 then
                    dragging = false;
                end;
            end);

            SliderExample.InputBegan:Connect(function(input)
                if slidng == true then
                    if input.UserInputType == Enum.UserInputType.MouseButton1 then
                        slide(input);
                    end;
                end
            end);

            game:GetService("UserInputService").InputChanged:Connect(function(input)
                if slidng == true then
                    if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
                        slide(input);
                    end;
                end
            end);
        end

        function ExtraLibrary:CreateDropdown(text, list, callback)
            text = text or "Dropdown"
            callback = callback or function(...) end
            list = list or {}
            
            local selected = list[1]

            local ExampleDropDown = Instance.new("TextButton")
            local ButtonExampleDropDown_BackGround = Instance.new("ImageLabel")
            local DropDownButton = Instance.new("ImageButton")
            local DropDrownArrow = Instance.new("ImageLabel")
            local Roundify = Instance.new("ImageLabel")
            local ExampleDropDownFrame = Instance.new("ImageLabel")
            local UIListLayout22 = Instance.new("UIListLayout")
            local UIPaddingTab22 = Instance.new("UIPadding")

            ExampleDropDown.Name = "ExampleDropDown"
            ExampleDropDown.Parent = ExampleTab
            ExampleDropDown.BackgroundColor3 = Color3.fromRGB(34, 34, 34)
            ExampleDropDown.BackgroundTransparency = 1.000
            ExampleDropDown.BorderSizePixel = 0
            ExampleDropDown.Position = UDim2.new(0.0536923148, 0, 0.0670926496, 0)
            ExampleDropDown.Size = UDim2.new(0, 438, 0, 30)
            ExampleDropDown.ZIndex = 2
            ExampleDropDown.Font = Enum.Font.GothamBold
            ExampleDropDown.Text = "  " ..text.. " :"
            ExampleDropDown.TextColor3 = Theme.TextColor
            ExampleDropDown.TextSize = 13.000
            ExampleDropDown.TextXAlignment = Enum.TextXAlignment.Left

            ButtonExampleDropDown_BackGround.Name = "ButtonExampleDropDown_BackGround"
            ButtonExampleDropDown_BackGround.Parent = ExampleDropDown
            ButtonExampleDropDown_BackGround.Active = true
            ButtonExampleDropDown_BackGround.AnchorPoint = Vector2.new(0.5, 0.5)
            ButtonExampleDropDown_BackGround.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            ButtonExampleDropDown_BackGround.BackgroundTransparency = 1.000
            ButtonExampleDropDown_BackGround.Position = UDim2.new(0.5, 0, 0.5, 0)
            ButtonExampleDropDown_BackGround.Selectable = true
            ButtonExampleDropDown_BackGround.Size = UDim2.new(1, 0, 1, 0)
            ButtonExampleDropDown_BackGround.Image = "rbxassetid://3570695787"
            ButtonExampleDropDown_BackGround.ImageColor3 = Theme.ButtonExampleDropDown_BackGround 
            ButtonExampleDropDown_BackGround.ScaleType = Enum.ScaleType.Slice
            ButtonExampleDropDown_BackGround.SliceCenter = Rect.new(100, 100, 100, 100)
            ButtonExampleDropDown_BackGround.SliceScale = 0.050

            DropDownButton.Name = "DropDownButton"
            DropDownButton.Parent = ExampleDropDown
            DropDownButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            DropDownButton.BackgroundTransparency = 1.000
            DropDownButton.Size = UDim2.new(0, 438, 0, 30)
            DropDownButton.ZIndex = 3
            DropDownButton.ScaleType = Enum.ScaleType.Fit

            DropDrownArrow.Name = "DropDrownArrow"
            DropDrownArrow.Parent = ExampleDropDown
            DropDrownArrow.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            DropDrownArrow.BackgroundTransparency = 1.000
            DropDrownArrow.BorderSizePixel = 0
            DropDrownArrow.Position = UDim2.new(0.914209664, 0, 0.0333333388, 0)
            DropDrownArrow.Size = UDim2.new(0, 27, 0, 27)
            DropDrownArrow.ZIndex = 4
            DropDrownArrow.Image = "rbxassetid://3944675151"

            Roundify.Name = "Roundify"
            Roundify.Parent = ExampleDropDown
            Roundify.AnchorPoint = Vector2.new(0.5, 0.5)
            Roundify.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Roundify.BackgroundTransparency = 1.000
            Roundify.Position = UDim2.new(0.944242597, 0, 0.5, 0)
            Roundify.Size = UDim2.new(0, 27, 0, 27)
            Roundify.ZIndex = 3
            Roundify.Image = "rbxassetid://3570695787"
            Roundify.ImageColor3 = Color3.fromRGB(39, 39, 39)
            Roundify.ScaleType = Enum.ScaleType.Slice
            Roundify.SliceCenter = Rect.new(100, 100, 100, 100)
            Roundify.SliceScale = 0.050

            ExampleDropDownFrame.Name = "ExampleDropDownFrame"
            ExampleDropDownFrame.Parent = ExampleTab
            ExampleDropDownFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            ExampleDropDownFrame.BackgroundTransparency = 1.000
            ExampleDropDownFrame.ClipsDescendants = true
            ExampleDropDownFrame.Position = UDim2.new(0.049788136, 0, 0.587662339, 0)
            ExampleDropDownFrame.Size = UDim2.new(0, 425, 0, 0)
            ExampleDropDownFrame.Image = "rbxassetid://3570695787"
            ExampleDropDownFrame.ImageColor3 = Theme.ExampleDropDownFrame
            ExampleDropDownFrame.ScaleType = Enum.ScaleType.Slice
            ExampleDropDownFrame.SliceCenter = Rect.new(100, 100, 100, 100)
            ExampleDropDownFrame.SliceScale = 0.050
            
            UIListLayout22.Parent = ExampleDropDownFrame
            UIListLayout22.SortOrder = Enum.SortOrder.LayoutOrder
            UIListLayout22.Padding = UDim.new(0, 3)
            
            UIPaddingTab22.Name = "UIPaddingTab22"
            UIPaddingTab22.Parent = ExampleDropDownFrame
            UIPaddingTab22.PaddingLeft = UDim.new(0, 3)
            UIPaddingTab22.PaddingTop = UDim.new(0, 3)

            ClickEffect(DropDownButton)

            local DropToggled = false

            for i,v in next, list do
                local ButtonExampleDropDown = Instance.new("TextButton")
                local ButtonExampleDropDown_Background = Instance.new("ImageLabel")
                ButtonExampleDropDown.Name = "ButtonExampleDropDown"
                ButtonExampleDropDown.Parent = ExampleDropDownFrame
                ButtonExampleDropDown.BackgroundColor3 = Color3.fromRGB(47, 47, 47)
                ButtonExampleDropDown.BackgroundTransparency = 1.000
                ButtonExampleDropDown.BorderSizePixel = 0
                ButtonExampleDropDown.Size = UDim2.new(0, 417, 0, 30)
                ButtonExampleDropDown.ZIndex = 2
                ButtonExampleDropDown.Font = Enum.Font.GothamBold
                ButtonExampleDropDown.Text = "  ".. tostring(v)
                ButtonExampleDropDown.TextColor3 = Theme.TextColor
                ButtonExampleDropDown.TextSize = 13.000
                ButtonExampleDropDown.TextXAlignment = Enum.TextXAlignment.Left
                
                ButtonExampleDropDown_Background.Name = "ButtonExampleDropDown_Background"
                ButtonExampleDropDown_Background.Parent = ButtonExampleDropDown
                ButtonExampleDropDown_Background.Active = true
                ButtonExampleDropDown_Background.AnchorPoint = Vector2.new(0.5, 0.5)
                ButtonExampleDropDown_Background.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                ButtonExampleDropDown_Background.BackgroundTransparency = 1.000
                ButtonExampleDropDown_Background.Position = UDim2.new(0.5, 0, 0.5, 0)
                ButtonExampleDropDown_Background.Selectable = true
                ButtonExampleDropDown_Background.Size = UDim2.new(1, 0, 1, 0)
                ButtonExampleDropDown_Background.Image = "rbxassetid://3570695787"
                ButtonExampleDropDown_Background.ImageColor3 = Theme.ButtonExampleDropDown_Background 
                ButtonExampleDropDown_Background.ScaleType = Enum.ScaleType.Slice
                ButtonExampleDropDown_Background.SliceCenter = Rect.new(100, 100, 100, 100)
                ButtonExampleDropDown_Background.SliceScale = 0.050
                ClickEffect(ButtonExampleDropDown)
                ExampleTab.CanvasSize = UDim2.new(0,0,0, UIListLayoutTab.AbsoluteContentSize.Y + 20)
            end

            for i,v in pairs(ExampleDropDownFrame:GetChildren()) do
                if v:IsA("TextButton") then
                    v.MouseButton1Down:Connect(function()
                        selected = v.Text
                        local str = selected
                        ExampleDropDown.Text = "  "..text .. " :" ..selected
                        pcall(callback, str:sub(3))
                        ExampleDropDownFrame:TweenSize(UDim2.new(0, 425,0, 0), "InOut", "Quad", 0.5, true)
                        DropToggled = false
                        game:GetService("TweenService"):Create(DropDownButton, TweenInfo.new(0.5), {Rotation = 0}):Play()
                        ExampleTab.CanvasSize = UDim2.new(0,0,0, UIListLayoutTab.AbsoluteContentSize.Y + 20)
                        wait(0.3)
                    end)
                end
            end
            
            DropDownButton.MouseButton1Click:Connect(function()
                ExampleDropDownFrame.Visible = true
                if DropToggled == false then
                    ExampleDropDownFrame:TweenSize(UDim2.new(0, 425,0 , UIListLayout22.AbsoluteContentSize.Y + 10), "InOut", "Quad", 0.5, true)
                    game:GetService("TweenService"):Create(DropDrownArrow, TweenInfo.new(0.5), {Rotation = 90}):Play()
                    ExampleTab.CanvasSize = UDim2.new(0,0,0, UIListLayoutTab.AbsoluteContentSize.Y + 20)
                else
                    game:GetService("TweenService"):Create(DropDrownArrow, TweenInfo.new(0.5), {Rotation = 0}):Play()
                    ExampleDropDownFrame:TweenSize(UDim2.new(0, 425,0, 0), "InOut", "Quad", 0.5, true)
                    wait(0.3)
                    ExampleTab.CanvasSize = UDim2.new(0,0,0, UIListLayoutTab.AbsoluteContentSize.Y + 20)
                end
                DropToggled = not DropToggled
            end)
        end

        function ExtraLibrary:CreateBind(text, starter, callback)
